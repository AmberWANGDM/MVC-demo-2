iginal":{"line":73,"column":6}},{"generated":{"line":78,"column":0},"source":"app1.js","original":{"line":74,"column":5}},{"generated":{"line":78,"column":5},"source":"app1.js","original":{"line":71,"column":4}},{"generated":{"line":79,"column":0},"source":"app1.js","original":{"line":75,"column":3}},{"generated":{"line":80,"column":0},"source":"app1.js","original":{"line":42,"column":10}},{"generated":{"line":80,"column":1},"source":"app1.js","original":{"line":42,"column":0}},{"name":"c","generated":{"line":82,"column":18},"source":"app1.js","original":{"line":78,"column":15}}],"sources":{"app1.js":"import $ from 'jquery'\nimport './app1.css'\n\n// ----------------- 数据相关 m -----------------\nconst m = {\n  // 数据\n  data:{\n    // 初始化数据\n     n : parseInt(localStorage.getItem('res'))\n  }\n}\n\n// ----------------- 视图相关 v -----------------\nconst v ={\n  // 容器\n  el:null,\n  // html字符串\n  html:`\n    <div>  \n        <div class=\"result\">计算结果: <span id=\"resultNum\">{{n}}</span></div>\n        <button class=\"btn add\">加1</button>\n        <button class=\"btn sub\">减1</button>\n        <button class=\"btn mul\">乘2</button>\n        <button class=\"btn div\">除2</button>\n    </div>\n  `,\n  // 初始化页面 & 保存container\n  init(container){\n    v.el = $(container)\n    v.render()\n  },\n  // 渲染 & 更新页面\n  render(){\n    if(v.el.children.length !== 0){\n      // 清空当前html,重新渲染\n      v.el.empty()\n    }\n    $(v.html.replace('{{n}}',m.data.n.toString())).appendTo(v.el)\n  }\n}\n// ----------------- 其它 c -----------------\nconst c = {\n  // 初始化, 形参为容器\n  init(container){\n    v.init(container)\n    c.ui={\n      // 需要的重要元素\n      button1 : $('.add'),\n      button2 : $('.sub'),\n      button3:  $('.mul'),\n      button4:  $('.div'),\n      number: $('#resultNum')\n    }\n    c.bindEvents()\n  },\n  // 绑定事件\n  bindEvents(){\n    // 绑定鼠标事件, 如果绑定在按钮上,更新页面后事件会失效,需要绑在container上\n    v.el.on('click','.add', () => {\n      m.data.n += 1\n      v.render()\n    })\n    v.el.on('click','.sub', () => {\n      m.data.n -= 1\n      v.render()\n    })\n    v.el.on('click','.mul', () => {\n      m.data.n *= 2\n      v.render()\n    })\n    v.el.on('click','.div', () => {\n      m.data.n /= 2\n      v.render()\n    })\n  }\n}\n\nexport default c\n\n\n\n\n\n\n\n\n\n"},"lineCount":null}},"hash":"7f59c5979f8b64fdeb8750a1bffc4a07","cacheData":{"env":{}}}